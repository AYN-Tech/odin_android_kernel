				//pins = "gpio129";         
				drive-strength = <2>;  
				output-low;  
			};
		};
	};
	
	usb2_3v3_en {
		usb2_3v3_en_default: usb2_3v3_en_default  {
			mux {
				//pins = "gpio128";
				function = "gpio";
			};

			config {
				//pins = "gpio128";         
				drive-strength = <2>;  
				output-high;  
			};
		};
	};

	usb2_1v2_en {
		usb2_1v2_en_default: usb2_1v2_en_default  {
			mux {
				pins = "gpio135";
				function = "gpio";
			};

